<?xml version="1.0"?>
<rdf:RDF xmlns="http://spider.sigappfr.org/linkedmdr/#"
     xml:base="http://spider.sigappfr.org/linkedmdr/"
     xmlns:dc="http://purl.org/dc/terms/"
     xmlns:ifc="https://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL/index.html#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:tei="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:lmdr="http://spider.sigappfr.org/linkedmdr/#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:mpeg7="http://mpeg7.org/#">
    <owl:Ontology rdf:about="http://spider.sigappfr.org/linkedmdr/#">
        <dc:creator>Nathalie Charbel</dc:creator>
        <dc:title>LinkedMDR</dc:title>
        <preferredNamespacePrefix xml:lang="en">LinkedMDR stands for Linked Multimedia Document Representation. 
It represents the collective knowledge embedded in a heterogeneous document corpus  of any domain-specific application.
It is made of three main layers: 
(1) the core layer serving as a backbone and a mediator among the different layers; 
(2) the metadata layer linking descriptors of existing standards in metadata representation; 
(3) the pluggable domain-specific layer that can adapt to any domain-specific ontology.
This version of LinkedMDR is based on descriptors from DC, TEI  and MPEG-7 standards for the standardized metadata layer and an adapted version of the ifcOWL ontology  for the pluggable domain-specific layer,</preferredNamespacePrefix>
        <preferredNamespacePrefix>lmdr</preferredNamespacePrefix>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#preferredNamespacePrefix"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/hasFormat -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/hasFormat">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#SyntacticRelation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">Describes a related document or media that is substantially the same as the current document or media, but in another format</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/hasVersion -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/hasVersion">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#SyntacticRelation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">Describes characteristics related to the form of the object</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/isReferenceBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isReferenceBy">
        <owl:equivalentProperty rdf:resource="http://spider.sigappfr.org/linkedmdr/#isReferencedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/isRequiredBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isRequiredBy">
        <owl:equivalentProperty rdf:resource="http://spider.sigappfr.org/linkedmdr/#isRequiredBy"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/references -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/references">
        <owl:equivalentProperty rdf:resource="http://spider.sigappfr.org/linkedmdr/#references"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/requires -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/requires">
        <owl:equivalentProperty rdf:resource="http://spider.sigappfr.org/linkedmdr/#requires"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/source -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/source">
        <owl:equivalentProperty rdf:resource="http://spider.sigappfr.org/linkedmdr/#source"/>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#DirectionalRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#DirectionalRelation">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#SpatialRelation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Describes the position of an object w.r.t. another object in space </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#OrderRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#OrderRelation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#Media"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#MediaComponent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#Media"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#MediaComponent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Describes a positioning order relation between two media or a media components</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#SemanticRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#SemanticRelation">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:comment xml:lang="en">Describes semantic characteristics related to the content of the object</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#SpatialRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#SpatialRelation">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:comment xml:lang="en">Describes relations between objects in space</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#SyntacticRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#SyntacticRelation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#Document"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#Media"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#Document"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#Media"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Describes characteristics related to the form of the document or the media</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#TemporalRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#TemporalRelation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:comment xml:lang="en">Describes the ordering in time between two objects</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#TopologicalRelation -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#TopologicalRelation">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#SpatialRelation"/>
        <rdfs:comment xml:lang="en">Describes structural relations between objects in space</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#annotates -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#annotates">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isAnnotatedBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://mpeg7.org/#StillRegion"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#TextElement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#ImageAnnotation"/>
        <rdfs:comment xml:lang="en">Describes an annotation of an image</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#appliesTo -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#appliesTo">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasChange"/>
        <rdfs:domain rdf:resource="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#change"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:comment xml:lang="en">Describes a relation between changes applied to an object and the latter</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#comprises -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#comprises">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isOn"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#lb"/>
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#pb"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#TextElement"/>
        <rdfs:comment xml:lang="en">Describes a containmenet relation between a page or a line and elements in the text</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#describes -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#describes">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isAssociatedTo"/>
        <rdfs:domain rdf:resource="http://mpeg7.org/#SemanticDescription"/>
        <rdfs:range rdf:resource="http://mpeg7.org/#StillRegion"/>
        <rdfs:comment xml:lang="en">Represents a relation between the description associated to an image region and the latter</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#disjoints -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#disjoints">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#TopologicalRelation"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">Describes a relation between two obejcts that have no common elements in space </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasChange -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasChange">
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:range rdf:resource="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#change"/>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasIfcProperty -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasIfcProperty">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isIfcPropertyOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL/index.html#IfcBuildingElement"/>
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL/index.html#IfcDistributionElement"/>
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL/index.html#IfcOpeningElement"/>
                    <rdf:Description rdf:about="https://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL/index.html#IfcSpatialElement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://standards.buildingsmart.org/IFC/DEV/IFC4_1/OWL/index.html#IfcProperty"/>
        <rdfs:comment xml:lang="en">Describes a relation between a property and an IFC concept. It replaces complex relations between a user defined property and a concept in IFC.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart">
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between objects</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_Object -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_Object">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart"/>
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isPartOf_Object"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:range rdf:resource="http://spider.sigappfr.org/linkedmdr/#Object"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between two objects</rdfs:comment>
        <rdfs:label xml:lang="fr">hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7 -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart"/>
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isPartOf_mpeg7"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 elements</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7Image -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7Image">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7"/>
        <rdfs:domain rdf:resource="http://spider.sigappfr.org/linkedmdr/#Image"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://mpeg7.org/#SpatialMask"/>
                    <rdf:Description rdf:about="http://mpeg7.org/#StillRegion"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#ImageAnnotation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 Image concept and any direct image component such as Spatial Mask or Still Region or Image Annotation</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7Polygon -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7Polygon">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7"/>
        <rdfs:domain rdf:resource="http://mpeg7.org/#Polygon"/>
        <rdfs:range rdf:resource="http://mpeg7.org/#Coordinate"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 Polygon concept and any direct polygon component such as Coordinate</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7SemanticDescription -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7SemanticDescription">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7"/>
        <rdfs:domain rdf:resource="http://mpeg7.org/#SemanticDescription"/>
        <rdfs:range rdf:resource="http://mpeg7.org/#Object"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 Semantic Description concept and the object it describes</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7SpatialMask -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7SpatialMask">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7"/>
        <rdfs:domain rdf:resource="http://mpeg7.org/#SpatialMask"/>
        <rdfs:range rdf:resource="http://mpeg7.org/#SubRegion"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 Spatial Mask and its related sub-region</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7StillRegion -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7StillRegion">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7"/>
        <rdfs:domain rdf:resource="http://mpeg7.org/#StillRegion"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://mpeg7.org/#SemanticDescription"/>
                    <rdf:Description rdf:about="http://mpeg7.org/#VisualDescriptor"/>
                    <rdf:Description rdf:about="http://spider.sigappfr.org/linkedmdr/#ImageAnnotation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 Still Region concept and any direct still region component such as the semantic description or the visual descriptor or the image annotation </rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7SubRegion -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7SubRegion">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_mpeg7"/>
        <rdfs:domain rdf:resource="http://mpeg7.org/#SubRegion"/>
        <rdfs:range rdf:resource="http://mpeg7.org/#Polygon"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between mpeg-7 sub-region concept and any direct sub-region component such as the polygon</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_tei -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_tei">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart"/>
        <owl:inverseOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#isPartOf_tei"/>
        <rdfs:comment xml:lang="en">Describes a whole part relation between TEI elements</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_teiCell -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_teiCell">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_tei"/>
        <rdfs:domain rdf:resource="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#cell"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#figure"/>
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#formula"/>
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#s"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Describes a whole part relation between a TEI table Cell concept and any direct cell component such as figure or formula or sentence</rdfs:comment>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://spider.sigappfr.org/linkedmdr/#hasPart_teiDiv -->

    <owl:ObjectProperty rdf:about="http://spider.sigappfr.org/linkedmdr/#hasPart_teiDiv">
        <rdfs:subPropertyOf rdf:resource="http://spider.sigappfr.org/linkedmdr/#hasPart_tei"/>
        <rdfs:domain rdf:resource="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#div"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#divGen"/>
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#figure"/>
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#head"/>
                    <rdf:Description rdf:about="http://www.tei-c.org/release/doc/tei-p5-doc/en/html/#list"/>
